{
  "functions": {
    "app/api/stripe/webhook/route.ts": {
      "maxDuration": 30
    },
    "app/api/stripe/webhook/test/route.ts": {
      "maxDuration": 10
    },
    "app/api/stripe/webhook/test-credit/route.ts": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "x-vercel-protection-bypass",
          "value": "disable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/messages/:id",
      "destination": "/api/messages/[id]"
    },
    {
      "source": "/api/messages/:id/read",
      "destination": "/api/messages/[id]/read"
    },
    {
      "source": "/api/listings/:id",
      "destination": "/api/listings/[id]"
    },
    {
      "source": "/api/listings/:id/favorite",
      "destination": "/api/listings/[id]/favorite"
    },
    {
      "source": "/api/listings/:id/is-favorite",
      "destination": "/api/listings/[id]/is-favorite"
    },
    {
      "source": "/api/listings/:id/reviews",
      "destination": "/api/listings/[id]/reviews"
    },
    {
      "source": "/api/listings/:id/stats",
      "destination": "/api/listings/[id]/stats"
    },
    {
      "source": "/api/listings/:id/unfavorite",
      "destination": "/api/listings/[id]/unfavorite"
    },
    {
      "source": "/api/listings/:id/view",
      "destination": "/api/listings/[id]/view"
    },
    {
      "source": "/api/business/:id",
      "destination": "/api/business/[id]"
    },
    {
      "source": "/api/business/:id/reviews",
      "destination": "/api/business/[id]/reviews"
    },
    {
      "source": "/api/business/:id/stats",
      "destination": "/api/business/[id]/stats"
    },
    {
      "source": "/api/reviews/:id/open",
      "destination": "/api/reviews/[id]/open"
    },
    {
      "source": "/api/admin/categories/:id",
      "destination": "/api/admin/categories/[id]"
    },
    {
      "source": "/api/admin/listings/:id",
      "destination": "/api/admin/listings/[id]"
    },
    {
      "source": "/api/admin/subcategories/:id",
      "destination": "/api/admin/subcategories/[id]"
    },
    {
      "source": "/api/admin/subscriptions/:id",
      "destination": "/api/admin/subscriptions/[id]"
    },
    {
      "source": "/api/admin/users/:id",
      "destination": "/api/admin/users/[id]"
    },
    {
      "source": "/api/seller/:id/response-time",
      "destination": "/api/seller/[id]/response-time"
    }
  ]
}
